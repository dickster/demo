+        List<FieldFeedbackMessage> reporters = Lists.newArrayList();
+        for (String inputId : inputMessageMap.keySet()) {
+            reporters.add(new FieldFeedbackMessage(inputId, inputMessageMap.get(inputId)));
+        }
+        return reporters;
+    }
+
+
+    class FieldFeedbackMessage {
+        String reporter;
+        String msg;
+
