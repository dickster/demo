+<wicket:panel>
+    <wicket:enclosure child="text">
+        <input type="text" wicket:id="text" class="txt form-control"/>
+        <input wicket:id="latitude" class="lat" type="hidden"/>
+        <input wicket:id="longitude" class="lng" type="hidden"/>
